{
  "name": "phil81334-ph-gsc",
  "displayName": "PH-GSC Custom Syntax Highlighter",
  "description": "\"Syntax highlighter for .ph_gsc files, enhancing code readability for GSC-to-Python-like syntax conversions.\"",
  "version": "0.0.2",
  "publisher": "Phil81334",
  "repository": {
    "type": "git",
    "url": "https://github.com/Phils-Hub-Org/Gsc_to_Py-like_syntax_highlighter.git"
  },
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "phil81334-ph-gsc.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "ph_gsc",
        "aliases": ["PH-GSC", "ph_gsc"],
        "extensions": [".ph_gsc"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ph_gsc",
        "scopeName": "source.ph_gsc",
        "path": "./syntaxes/ph_gsc.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
